#import "UserInfoUnit.h"

@implementation UserInfoUnit

-(id)initWithSign:(NSString *)sign content:(NSString *)content andEditable:(BOOL)yn{
    self = [super init];
    if(self){
        _sign = sign;
        _signContent = content;
        _isEditTable = yn;
    }
    return self;
    
}

@end
