#import <Foundation/Foundation.h>

@interface ProjectModel : JSONModel

@property (nonatomic,strong) NSString *projid;   //id
@property (nonatomic,strong) NSString *cost;
@property (nonatomic,strong) NSString *got_update;

@property (nonatomic,strong) NSString *is_new_publish;
@property (nonatomic,strong) NSString *item_description;
@property (nonatomic,strong) NSString *name;
@property (nonatomic,strong) NSString *stage;
@property (nonatomic,strong) NSString *time_lastupdate;

@end


/*
 "next_page：下一页的页码，对应下一次请求的page；
 record_count：搜索到的总记录数；
 items：搜索的结果列表
 
    id：工程信息编号；
    name：项目名；
    time_lastupdate：最后更新时间；
    item_description：信息描述（联系人数量情况）；
    stage：项目阶段；
    cost：工程造价；"
 
 
 "next_page：下一页的页码，对应下一次请求的page；
 update_count：自从上次退出APP以来更新的订阅数；
 items：订阅信息的列表
    id：工程信息ID"
    name：工程标题；
    time_lastupdate：最后更新时间（yyyy年MM月）；
    stage：工程阶段；
    cost：工程造价；
 
    got_update：自从上次退出APP以来是否有更新（0：无更新，1有更新）；
    is_new_publish：是否为新发布信息；
    item_description：信息描述（联系人数量情况）；
 */





